{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "../../out/dist/renderer",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "exactOptionalPropertyTypes": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "paths": {
      "$ipc/core": ["../ipc/core/index.mjs"],
      "$ipc/main-renderer": ["../ipc/main-renderer/index.mjs"],
      "$renderer/*": ["./*/index.mjs"],
      "$renderer/components": ["./ui/components.mjs"],
      "$renderer/dux": ["./dux/index.mjs"],
      "$renderer/dux/*": ["./dux/*.mjs"]
    }
  },
  "include": ["./**/*.mts", "./**/*.tsx", "./**/*.svg", "./**/*.json"],
  "references": [
    {
      "path": "../ipc/tsconfig.json"
    }
  ]
}
